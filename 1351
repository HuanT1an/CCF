class Solution {
public:
    int countNegatives(vector<vector<int>>& grid) {
    int i,j;
    int t=0;
    int m=grid.size(),n=grid[i].size();
    for(i=m-1;i>=0;i--){
        for(j=n-1;j>=0;j--){
            if(grid[i][j]<0){
                t++;
        }
        else{
            continue;
        }
    }
}
return t;
    }
};
